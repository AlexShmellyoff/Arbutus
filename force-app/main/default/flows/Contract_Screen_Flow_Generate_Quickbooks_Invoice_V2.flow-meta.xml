    <formulas>
        <name>CycleOverageDescription</name>
        <dataType>String</dataType>
        <expression>&quot;Overage for the: &quot; + {!Get_Subscription_Quote_Line.SBQQ__Description__c} + 
IF(NOT(ISBLANK({!CurrentSubscriptionStatus})), 
   &quot; | Status: &quot; + {!CurrentSubscriptionStatus}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c})), 
   &quot; | Return Date: &quot; + {!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.SBQQ__StartDate__c})), 
   &quot; | Start Date: &quot; + TEXT({!Get_Subscription_Quote_Line.SBQQ__StartDate__c}), 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Expected_End_Date__c})), 
   &quot; | End Date: &quot; + TEXT({!Get_Subscription_Quote_Line.Expected_End_Date__c}), 
   &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>EnhancedStandardDescription</name>
        <dataType>String</dataType>
        <expression>{!Get_Subscription_Quote_Line.SBQQ__Description__c} + 
IF(NOT(ISBLANK({!CurrentSubscriptionStatus})), 
   &quot; | Status: &quot; + {!CurrentSubscriptionStatus}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c})), 
   &quot; | Return Date: &quot; + {!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.SBQQ__StartDate__c})), 
   &quot; | Start Date: &quot; + TEXT({!Get_Subscription_Quote_Line.SBQQ__StartDate__c}), 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Expected_End_Date__c})), 
   &quot; | End Date: &quot; + TEXT({!Get_Subscription_Quote_Line.Expected_End_Date__c}), 
   &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>OverageDescription</name>
        <dataType>String</dataType>
        <expression>{!Get_Subscription_Quote_Line.SBQQ__Description__c} + 
IF(NOT(ISBLANK({!CurrentSubscriptionStatus})), 
   &quot; | Status: &quot; + {!CurrentSubscriptionStatus}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c})), 
   &quot; | Return Date: &quot; + {!Get_Subscription_Quote_Line.Related_Subscription_Return_Date__c}, 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.SBQQ__StartDate__c})), 
   &quot; | Start Date: &quot; + TEXT({!Get_Subscription_Quote_Line.SBQQ__StartDate__c}), 
   &quot;&quot;) +
IF(NOT(ISBLANK({!Get_Subscription_Quote_Line.Expected_End_Date__c})), 
   &quot; | End Date: &quot; + TEXT({!Get_Subscription_Quote_Line.Expected_End_Date__c}), 
   &quot;&quot;) + 
&quot; + Overage Amount of: $&quot; + TEXT(ROUND({!Loop_Through_Returned_Subscriptions.Overage_Amount__c}, 2))</expression>
    </formulas>
    <formulas>
        <name>TotalOverage</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Subscription_Quote_Line.SBQQ__NetTotal__c} + {!Loop_Through_Returned_Subscriptions.Overage_Amount__c}</expression>
        <scale>2</scale>
    </formulas>
