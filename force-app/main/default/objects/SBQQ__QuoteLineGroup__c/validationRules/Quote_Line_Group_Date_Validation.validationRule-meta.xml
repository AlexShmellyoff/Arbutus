<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quote_Line_Group_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(SBQQ__StartDate__c)),
    NOT(ISBLANK(SBQQ__EndDate__c)),
    SBQQ__EndDate__c &lt;= SBQQ__StartDate__c
)</errorConditionFormula>
    <errorDisplayField>SBQQ__EndDate__c</errorDisplayField>
    <errorMessage>End Date must be after the Start Date for this Quote Line Group.</errorMessage>
</ValidationRule>
